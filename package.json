{
  "name": "lesspass-pure",
  "version": "4.5.6",
  "description": "pure LessPass component for LessPass with vuejs",
  "license": "GPL-3.0",
  "author": "Guillaume Vincent <guillaume@oslab.fr>",
  "repository": "lesspass/pure",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "precommit": "npm test",
    "prepush": "npm test",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack -p",
    "build:i18n": "cd scripts && node buildI18n.js",
    "dev": "webpack-dev-server --inline --hot --host 0.0.0.0",
    "test": "ava test --compilers js:babel-register"
  },
  "babel": {
    "comments": false,
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "dependencies": {
    "awesomplete": "^1.1.1",
    "axios": "^0.16.1",
    "bootstrap": "4.0.0-alpha.6",
    "clipboard": "^1.6.1",
    "font-awesome": "^4.7.0",
    "hint.css": "^2.4.1",
    "jwt-decode": "^2.2.0",
    "lesspass": "^6.0.0",
    "lodash.debounce": "^4.0.8",
    "vue": "^2.2.6",
    "vue-polyglot": "^0.1.7",
    "vue-router": "^2.4.0",
    "vuex": "^2.3.1",
    "vuex-persistedstate": "^1.4.0",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "husky": "^0.13.3",
    "json-loader": "^0.5.4",
    "nock": "^9.0.13",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "timekeeper": "^1.0.0",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.3.4",
    "vue-polyglot-utils": "^0.1.1",
    "vue-template-compiler": "^2.2.6",
    "walk": "^2.3.9",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
